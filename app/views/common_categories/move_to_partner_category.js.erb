id = "<%= @category.id %>";
removeList = document.querySelector(`#category-list-id-${id}`);
parentElementId = '#partner-categories';
html = "<%= j(render partial: 'categories/a_partner_category_list', locals: { category: @category }) %>";
parentElement = document.querySelector(parentElementId);
list = document.createElement('li');
list.id = `category-list-id-${id}`;
list.innerHTML = html;
parentElement.appendChild(list);
removeList.outerHTML = '';