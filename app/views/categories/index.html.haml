%div.whole_page.container
  %h3 カテゴリー一覧
  - @common_categories.each do |category|
    %ul
      %li #{category.kind}

  - @my_categories.each do |category|
    %ul
      %li #{category.kind}

  %div
  = link_to '共通のカテゴリーの登録', common_categories_path, class: "btn btn-default"
  %div
  = link_to 'カテゴリー追加', new_category_path, class: "btn btn-default"
